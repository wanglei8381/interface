{
  "interfaces" : [
    {
      "name" : "interfaces/app/app.js",
      "path" : "/app",
      "method" : "ALL",
      "public" : true
    },
    {
      "name" : "interfaces/msg",
      "path" : "/msg",
      "method" : "GET,POST",
      "public" : false
    }
  ],
  "filters" : [
    {
      "mapping" : "/app",
      "exclude-mapping" : "/app/main/**",
      "target" :"/filter/mainFilter.js"
    },
    {
      "mapping" : ["/app","/app2"],
      "exclude-mapping" : ["/app/a","/app2/b"],
      "target" :"/filter/mainFilter.js"
    }
  ]
}