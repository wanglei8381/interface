{
  "interfaces" : [
    {
      "name" : "if_app",
      "path" : "app",
      "method" : "POST",
      "public" : true
    },
    {
      "name" : "sys/user",
      "path" : "sys/user",
      "method" : "GET,POST",
      "public" : false
    }
  ],
  "interceptors" : [
    {
      "mapping" : "/app",
      "exclude-mapping" : "/app/main/**",
      "target" :"/interceptor/mainInterceptor"
    },
    {
      "mapping" : ["/app","/app2"],
      "exclude-mapping" : ["/app/a","/app2/b"],
      "target" :"/interceptor/mainInterceptor"
    }
  ]
}